<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_homepage"
    android:layout_width="match_parent"
    android:layout_height="530dip"

    tools:context="bonaguasato.gui.homepage">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#616161">

        <ImageView
            android:layout_width="50dip"
            android:layout_height="50dip"
            app:srcCompat="@drawable/logo"
            android:id="@+id/imageView"
            android:adjustViewBounds="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            tools:ignore="ContentDescription,RtlHardcoded" />

    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#FCE4EC"
        android:layout_alignParentTop="true"
        android:layout_marginTop="50dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        tools:ignore="RtlHardcoded">

        <ImageButton
            android:background="@drawable/web"
            android:layout_width="100dip"
            android:layout_height="90dip"

            android:id="@+id/imgbtnWeb"
            android:layout_alignTop="@+id/imgbtnCamera"
            android:layout_centerHorizontal="true"
            tools:ignore="ContentDescription,NotSibling" />

        <ImageButton
            android:background="@drawable/location"
            android:layout_width="70dip"
            android:layout_height="60dip"
            android:adjustViewBounds="true"
            tools:ignore="ContentDescription"
            android:id="@+id/imgbtnlocation"
            android:layout_marginTop="71dp"
            android:layout_below="@+id/imgbtnWeb"
            android:layout_toStartOf="@+id/imgbtnWeb" />

        <ImageButton
            android:background="@drawable/scan"
            android:layout_width="100dip"
            android:layout_height="100dip"
            android:adjustViewBounds="true"
            tools:ignore="ContentDescription"
            android:id="@+id/imgbtnscan"
            android:layout_marginTop="49dp"
            android:layout_below="@+id/imgbtnWeb"
            android:layout_alignParentEnd="true"
            android:layout_marginEnd="53dp" />

    </RelativeLayout>

        <TextView
            android:text="User Profile"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_marginBottom="360dp"
            android:id="@+id/textView2"
            tools:textColor="@android:color/holo_blue_dark"
            android:textStyle="normal|bold"
            android:textAllCaps="true"
            android:textColor="@android:color/holo_blue_dark"
            tools:ignore="HardcodedText,RtlHardcoded" />

        <TextView
            android:text="BEHAVIOR VIEWER"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_centerHorizontal="true"
            android:layout_marginBottom="360dp"
            android:id="@+id/textView6"
            tools:textColor="@android:color/holo_blue_dark"
            android:textAllCaps="true"
            android:textStyle="normal|bold"
            android:textColor="@android:color/holo_blue_dark"
            tools:ignore="HardcodedText" />

        <TextView
            android:text="Camera"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="false"
            android:layout_marginRight="14dp"
            android:layout_marginEnd="14dp"
            android:layout_marginBottom="360dp"
            android:id="@+id/textView7"
            tools:textColor="@android:color/holo_blue_dark"
            android:layout_alignParentBottom="true"
            android:layout_below="@+id/imageButton13"
            android:textStyle="normal|bold"
            android:textAllCaps="true"
            android:textColor="@android:color/holo_blue_dark"
            tools:ignore="HardcodedText,RtlHardcoded,UnknownId" />

        <ImageButton
            android:background="@drawable/users"
            android:layout_width="100dip"
            android:layout_height="110dip"
            android:layout_alignParentBottom="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_marginBottom="367dp"
            android:id="@+id/imgbtnUser"
            android:soundEffectsEnabled="false"
            tools:soundEffectsEnabled="false"
            tools:ignore="ContentDescription,RtlHardcoded" />

    <ImageButton
            android:background="@drawable/eye"
            android:layout_width="100dip"
            android:layout_height="60dip"
            android:adjustViewBounds="true"
            android:layout_above="@+id/textView7"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true"
            android:id="@+id/imgbtnCamera"
        tools:ignore="ContentDescription,RtlHardcoded" />

    <TextView
        android:text="Location"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="76dp"
        android:textColor="@android:color/holo_blue_dark"
        android:textStyle="normal|bold"
        android:textAllCaps="true"
        tools:ignore="HardcodedText,UnknownId"
        android:id="@+id/textView8"
        android:layout_alignTop="@+id/textView2"
        android:layout_alignParentStart="true"
        android:layout_marginTop="129dp" />

    <TextView
        android:text="Text Scan"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@android:color/holo_blue_dark"
        android:textStyle="normal|bold"
        android:textAllCaps="true"
        tools:ignore="HardcodedText,UnknownId"
        android:id="@+id/textView9"
        android:layout_alignBaseline="@+id/textView8"
        android:layout_alignBottom="@+id/textView8"
        android:layout_toStartOf="@+id/textView7" />

</RelativeLayout>


